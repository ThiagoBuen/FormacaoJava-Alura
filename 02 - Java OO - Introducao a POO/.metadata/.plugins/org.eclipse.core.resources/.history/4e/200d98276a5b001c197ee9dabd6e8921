public class CriaConta {
	public static void main(String[] args) {
		Conta primeiraConta = new Conta();		//Cria (instancia) um objeto do tipo Conta
		primeiraConta.saldo = 200;
		System.out.println(primeiraConta.saldo);
		
		primeiraConta.saldo += 200;
		System.out.println(primeiraConta.saldo);
		
	}
}
